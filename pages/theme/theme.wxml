<!--pages/theme/theme.wxml-->
<import src='../tpls/products/products-tpl.wxml'/>

<view class='container theme-container'>
  <view class='theme-header'>
    <image src='{{baseImageUrl}}{{themeInfo.head_img.url}}'            mode='aspectFill'></image>
  </view>
  <view class='theme-main'>
    <template is='products' data='{{products:themeInfo.products,baseImageUrl}}'></template>
  </view>
</view>